        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.5"
    },
    "jest-junit": {
        "suiteName": "UnitTest",
        "outputDirectory": "./coverage",
        "outputName": "test-report.xml",
        "uniqueOutputName": "false",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}",
        "ancestorSeparator": " â€º ",
        "usePathForSuiteName": "true"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "./",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/dist/",
            "/coverage/",
            ".*\\.module\\.ts$",
            ".*\\.interceptor\\.ts$",
            "/src/main.ts",
            "src/controller/interceptor/.*",
            "./.eslintrc.js",
            "src/mockup/.*",
            "src/common/lib/exceptions-manager.filter.ts",
            "src/controller/dto/.*",
            "src/core/entity/.*",
            "src/core/use-case/resource/impl/service-error.resource.uc.impl.ts",
            "src/data-provider/model/.*",
            "src/data-provider/provider/http.provider.impl.ts"
        ],
        "testResultsProcessor": "jest-sonar-reporter"
    },
    "jestSonar": {
        "reportPath": "coverage",
        "reportFile": "test-reporter.xml",
        "indent": 4
    }
}
